<?xml version='1.0' encoding='UTF-8'?>
<hudson>
  <version>1.391</version>
  <numExecutors>1</numExecutors>
  <mode>NORMAL</mode>
  <useSecurity>true</useSecurity>
  <authorizationStrategy class="hudson.security.FullControlOnceLoggedInAuthorizationStrategy"/>
  <securityRealm class="hudson.security.HudsonPrivateSecurityRealm">
    <disableSignup>true</disableSignup>
  </securityRealm>
  <systemMessage>&lt;p&gt;The argouml-junit-javaX and the daily-userdoc job starts after a commit. If an error is found they send mails to the committer (and to the users(at)argouml-stats.tigris.org mailing list)&lt;/p&gt;&#xd;
&#xd;
&lt;p&gt;The argonightly-java jobs does the nightly build. It starts at midnight (if there is any change in any of the involved projects). Each job uploads the result to the &lt;a href=&quot;http://argouml-stats.tigris.org/&quot;&gt;argouml website&lt;/a&gt; separately. To limit the impact of these jobs, they are bound to the low priority-executor that runs niced.&lt;/p&gt;&#xd;
&#xd;
&lt;p&gt;All jobs fetches information from a local svn mirror of the tigris svn repositories that is updated regularly by the update-mirror jobs.&lt;/p&gt;&#xd;
&lt;p&gt;show-jobs fetches information from the repository and displays it using the hudson publishing mechanisms.&lt;/p&gt;&#xd;
</systemMessage>
  <markupFormatter class="hudson.markup.RawHtmlMarkupFormatter"/>
  <jdks>
    <jdk>
      <name>1.6</name>
      <home>/usr/local/lib/java/jdk1.6.0</home>
      <properties/>
    </jdk>
    <jdk>
      <name>1.5</name>
      <home>/usr/local/lib/java/jdk1.5.0_06</home>
      <properties/>
    </jdk>
    <jdk>
      <name>1.7</name>
      <home>/usr/local/lib/java/jdk1.7.0</home>
      <properties/>
    </jdk>
  </jdks>
  <viewsTabBar class="hudson.views.DefaultViewsTabBar"/>
  <myViewsTabBar class="hudson.views.DefaultMyViewsTabBar"/>
  <clouds/>
  <slaves>
    <slave>
      <name>prio1exec</name>
      <description></description>
      <remoteFS>/home2/argonightly/prio1exec</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>EXCLUSIVE</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Demand">
        <inDemandDelay>1</inDemandDelay>
        <idleDelay>5</idleDelay>
      </retentionStrategy>
      <launcher class="hudson.slaves.CommandLauncher">
        <agentCommand>nice -n 4 /usr/local/lib/java/jdk1.6.0/bin/java -Xms8M -jar /home2/argonightly/tools/WEB-INF/slave.jar</agentCommand>
      </launcher>
      <label>prio1</label>
      <nodeProperties/>
    </slave>
    <slave>
      <name>prio2exec</name>
      <description></description>
      <remoteFS>/home2/argonightly/prio2exec</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>EXCLUSIVE</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Demand">
        <inDemandDelay>3</inDemandDelay>
        <idleDelay>5</idleDelay>
      </retentionStrategy>
      <launcher class="hudson.slaves.CommandLauncher">
        <agentCommand>nice -n 8 /usr/local/lib/java/jdk1.6.0/bin/java -Xms8M -jar /home2/argonightly/tools/WEB-INF/slave.jar</agentCommand>
      </launcher>
      <label>prio1 prio2</label>
      <nodeProperties/>
    </slave>
    <slave>
      <name>prio3exec</name>
      <description></description>
      <remoteFS>/home2/argonightly/prio3exec</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>EXCLUSIVE</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Demand">
        <inDemandDelay>5</inDemandDelay>
        <idleDelay>5</idleDelay>
      </retentionStrategy>
      <launcher class="hudson.slaves.CommandLauncher">
        <agentCommand>nice -n 12 /usr/local/lib/java/jdk1.6.0/bin/java -Xms8M -jar /home2/argonightly/tools/WEB-INF/slave.jar</agentCommand>
      </launcher>
      <label>prio1 prio2 prio3</label>
      <nodeProperties/>
    </slave>
  </slaves>
  <quietPeriod>20</quietPeriod>
  <scmCheckoutRetryCount>0</scmCheckoutRetryCount>
  <views>
    <hudson.model.AllView>
      <owner class="hudson" reference="../../.."/>
      <name>All</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
    </hudson.model.AllView>
  </views>
  <primaryView>All</primaryView>
  <slaveAgentPort>0</slaveAgentPort>
  <label></label>
  <nodeProperties/>
  <globalNodeProperties/>
  <disabledAdministrativeMonitors>
    <string>hudson.node_monitors.MonitorMarkedNodeOffline</string>
  </disabledAdministrativeMonitors>
</hudson>