    <useUpdate>true</useUpdate>
    <doRevert>false</doRevert>
    <excludedRegions></excludedRegions>
    <includedRegions></includedRegions>
    <excludedUsers></excludedUsers>
    <excludedRevprop></excludedRevprop>
    <excludedCommitMessages></excludedCommitMessages>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.SCMTrigger>
      <spec>0 0 * * *</spec>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <axes>
    <axis>
      <name>type</name>
      <values>
        <string>junit</string>
        <string>javadocs</string>
        <string>release</string>
        <string>checkstyle</string>
        <string>findbugs</string>
        <string>jdepend</string>
        <string>coverage</string>
        <string>i18ncomparison</string>
      </values>
    </axis>
    <axis>
      <name>jdk</name>
      <values>
        <string>1.6</string>
        <string>1.5</string>
      </values>
    </axis>
    <axis>
      <name>label</name>
      <values>
        <string>nightly</string>
      </values>
    </axis>
  </axes>
  <combinationFilter>jdk==&quot;1.5&quot; || (type != &quot;checkstyle&quot; &amp;&amp; type != &quot;findbugs&quot; &amp;&amp; type != &quot;jdepend&quot; &amp;&amp; type != &quot;i18ncomparison&quot;)</combinationFilter>
  <builders>
    <hudson.tasks.Shell>
      <command>cd argouml-gen &amp;&amp; nice ./build.sh -Dworking.dir=.. report:$type
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>case &quot;$JAVA_HOME&quot; in
*jdk1.5.0*)
    JAVA_NAME=java5
    ;;
*jdk1.6.0*)
    JAVA_NAME=java6
    ;;
*)
    echo &quot;Unknown jdk!&quot;
    exit 1
    ;;
esac

for dir in RESULT/reports/$type*
do
nice argouml-gen/hudson/h-upload.sh http://argouml-stats.tigris.org/svn/argouml-stats/trunk/www/reports-$JAVA_NAME/`basename $dir` $dir
done
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd argouml-gen &amp;&amp; nice ./build.sh -Dworking.dir=.. clean-result</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <timeoutMinutes>1200</timeoutMinutes>
      <failBuild>false</failBuild>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
  </buildWrappers>
  <runSequentially>false</runSequentially>
  <touchStoneCombinationFilter>type == &quot;release&quot;</touchStoneCombinationFilter>
  <touchStoneResultCondition>
    <name>UNSTABLE</name>
    <ordinal>1</ordinal>
    <color>YELLOW</color>
  </touchStoneResultCondition>
</matrix-project>