<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Commits the rest after the nightly job.</description>
  <logRotator>
    <daysToKeep>25</daysToKeep>
    <numToKeep>-1</numToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <quietPeriod>300</quietPeriod>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <jdk>(Default)</jdk>
  <triggers class="vector"/>
  <builders>
    <hudson.tasks.Shell>
      <command>cd /home2/argonightly/argouml-gen &amp;&amp; ./stats-commit.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd /home2/argonightly/argouml-gen &amp;&amp; ./stats-commit.sh 100</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd /home2/argonightly/argouml-gen &amp;&amp; ./stats-commit.sh 10</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd /home2/argonightly/argouml-gen &amp;&amp; ./stats-commit.sh 1</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.textfinder.TextFinderPublisher>
      <regexp>Could not resolve hostname|timed out waiting for server|could not connect to server|commit attempts failed.*Giving up</regexp>
      <succeedIfFound>false</succeedIfFound>
      <unstableIfFound>true</unstableIfFound>
      <alsoCheckConsoleOutput>true</alsoCheckConsoleOutput>
    </hudson.plugins.textfinder.TextFinderPublisher>
  </publishers>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <timeoutMinutes>2400</timeoutMinutes>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
    <hudson.plugins.locksandlatches.LockWrapper>
      <locks>
        <hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
          <name>argouml-stats</name>
        </hudson.plugins.locksandlatches.LockWrapper_-LockWaitConfig>
      </locks>
    </hudson.plugins.locksandlatches.LockWrapper>
  </buildWrappers>
</project>